$TTL 86400
@	IN	SOA	ns1.vladimir.com. root.vladimir.com. (
	2013042201	;Serial
	3600	;Refresh
	1800	;Retry
	604800	;Expire
	86400	;Minimum TTL
)
; Specify our two nameservers
	IN	NS 	ns1.vladimir.com.

; Resolve nameserver hostnames to IP
ns1	IN	A	192.168.2.39

;Define hostname
@	IN	A	192.168.2.39
www	IN	A	192.168.2.39
dns	IN	A	192.168.2.39
examen	IN	A	192.168.8.8
